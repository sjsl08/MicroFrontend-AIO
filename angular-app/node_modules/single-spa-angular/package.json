{
  "$schema": "../node_modules/ng-packagr/ng-package.schema.json",
  "name": "single-spa-angular",
  "version": "8.1.1",
  "description": "Helpers for building single-spa applications which use Angular 2",
  "schematics": "./schematics/schematics.json",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/single-spa/single-spa-angular.git"
  },
  "keywords": [
    "single-spa",
    "angular"
  ],
  "author": "Joel Denning",
  "maintainers": [
    "Joel Denning",
    "Artur Androsovych"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/single-spa/single-spa-angular/issues"
  },
  "homepage": "https://github.com/single-spa/single-spa-angular#readme",
  "sideEffects": false,
  "peerDependencies": {
    "@angular/core": ">=15.1.0",
    "json5": "*",
    "single-spa": ">=4.0.0",
    "style-loader": "^3.3.1"
  },
  "exports": {
    "./lib/webpack": {
      "types": "./lib/webpack/index.d.ts",
      "default": "./lib/webpack/index.js"
    },
    "./package.json": {
      "default": "./package.json"
    },
    ".": {
      "types": "./index.d.ts",
      "esm2020": "./esm2020/single-spa-angular.mjs",
      "es2020": "./fesm2020/single-spa-angular.mjs",
      "es2015": "./fesm2015/single-spa-angular.mjs",
      "node": "./fesm2015/single-spa-angular.mjs",
      "default": "./fesm2020/single-spa-angular.mjs"
    },
    "./elements": {
      "types": "./elements/index.d.ts",
      "esm2020": "./esm2020/elements/single-spa-angular-elements.mjs",
      "es2020": "./fesm2020/single-spa-angular-elements.mjs",
      "es2015": "./fesm2015/single-spa-angular-elements.mjs",
      "node": "./fesm2015/single-spa-angular-elements.mjs",
      "default": "./fesm2020/single-spa-angular-elements.mjs"
    },
    "./internals": {
      "types": "./internals/index.d.ts",
      "esm2020": "./esm2020/internals/single-spa-angular-internals.mjs",
      "es2020": "./fesm2020/single-spa-angular-internals.mjs",
      "es2015": "./fesm2015/single-spa-angular-internals.mjs",
      "node": "./fesm2015/single-spa-angular-internals.mjs",
      "default": "./fesm2020/single-spa-angular-internals.mjs"
    },
    "./parcel": {
      "types": "./parcel/index.d.ts",
      "esm2020": "./esm2020/parcel/single-spa-angular-parcel.mjs",
      "es2020": "./fesm2020/single-spa-angular-parcel.mjs",
      "es2015": "./fesm2015/single-spa-angular-parcel.mjs",
      "node": "./fesm2015/single-spa-angular-parcel.mjs",
      "default": "./fesm2020/single-spa-angular-parcel.mjs"
    }
  },
  "module": "fesm2015/single-spa-angular.mjs",
  "es2020": "fesm2020/single-spa-angular.mjs",
  "esm2020": "esm2020/single-spa-angular.mjs",
  "fesm2020": "fesm2020/single-spa-angular.mjs",
  "fesm2015": "fesm2015/single-spa-angular.mjs",
  "typings": "index.d.ts",
  "dependencies": {
    "tslib": "^2.3.0"
  }
}